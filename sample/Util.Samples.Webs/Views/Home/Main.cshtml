<util-sidenav-container fullscreen="true">
    @*导航侧边栏*@
    <util-sidenav id="sidenav" opened="true" mode="Side" width="250">
        @Html.Partial( "Main/SideNav" )
    </util-sidenav>
    <util-sidenav-content>
        @*页头*@
        @Html.Partial( "Main/Header" )
        @*内容区*@
        <div class="navbar-content">
            <util-router-outlet></util-router-outlet>
        </div>
        @*页脚*@
        @Html.Partial( "Main/Footer" )
    </util-sidenav-content>
</util-sidenav-container>

@*消息提示*@
<p-growl life="5000"></p-growl>